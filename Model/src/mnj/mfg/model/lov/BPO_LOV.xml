  <SQLQuery>
    <![CDATA[select l.attribute1 BPO,
       l.attribute2 STN,
       l.attribute3 Color,
       l.quantity Quantity, 
       l.production_type Production_Type,
       l.wash_type Wash_Type,
       l.ISSUANCE_CHALLAN_NO,
       l.attribute7 Wash_Unwash
from MNJ_ISSUE_REC_WASH_DEL_L l
where (SELECT DISTINCT ac.CUSTOMER_ID
          FROM hz_cust_accounts hca, hz_parties hp, ar_customers ac
         WHERE     hp.PARTY_ID = hca.PARTY_ID
               AND hca.CUST_ACCOUNT_ID = ac.CUSTOMER_ID
               AND hp.Known_as = l.attribute6
               and hp.KNOWN_AS is not null
               and ac.STATUS = 'A'
               and ac.CUSTOMER_NAME_PHONETIC = 'M'
                and rownum  =1) = :P_Buyer
and l.attribute5 = :P_Style
and l.attribute4 = :P_Season]]>
  </SQLQuery>
