  <SQLQuery>
    <![CDATA[---/ Formatted on 10/14/2020 1:46:01 PM (QP5 v5.256.13226.35510) /
SELECT BPO_ID,
BPO,
SEASON,
STYLE,
COLOR,
WASH_NAME WASH,
TRANS_TYPE
FROM (SELECT DISTINCT V.BPO_ID,
V.BPO,
V.SEASON,
V.STYLE,
V.COLOR,
V.WASH_NAME,
'STICHING' TRANS_TYPE
FROM MNJ_MFG_LINEOUTPUT H,
MNJ_MFG_LINEOUTPUT_DETAIL DDD,
MNJ_MFG_LINEOUTPUT_SBLINE LL,
XX_ORDER_INFO_V V
WHERE LL.OUT_SUBLINE_ID = DDD.OUT_SUBLINE_ID
AND H.PROD_TRANS_ID = LL.PROD_TRANS_ID
AND LL.SYSTEM_ID = V.SYSTEM_ID
AND LL.BPO_ID = V.BPO_ID
UNION
SELECT DISTINCT V.BPO_ID,
V.BPO,
V.SEASON,
V.STYLE,
V.COLOR,
V.WASH_NAME,
'FINISHING' TRANS_TYPE
FROM MNJ_MFG_FINISH_PROD_H MMFH,
MNJ_MFG_FINISH_PROD_L MMFL,
--MNJ_MFG_FINISH_PROD_D MMFD,
XX_ORDER_INFO_V V
WHERE MMFH.HEADER_ID = MMFL.HEADER_ID AND V.BPO_ID = MMFL.BPO_ID)
WHERE TRANS_TYPE = :p_trans_type]]>
  </SQLQuery>
